import requests

# Define variables
origin = 'Origin Address'
destination = 'Destination Address'
increment = '25'

# Create URL
url = 'https://research.jamesworden.com/route?origin=' + origin + \
      '&destination=' + destination + '&increment=' + increment

# Use requests to send request
response = requests.request("GET", url, headers={
    'x-api-key': 'YOUR_KEY_HERE'
})
data = response.text.encode('utf8')

# Your code here
print(data)